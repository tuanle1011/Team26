
<!DOCTYPE html>
<html>
<head>

    @RenderSection("scripts", required: false)
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font awesome -->
    <link href=@Url.Content("/Content/css/font-awesome.css") rel="stylesheet">
    <!-- Bootstrap -->
    <link href=@Url.Content("/Content/css/bootstrap.css") rel="stylesheet">
    <!-- SmartMenus jQuery Bootstrap Addon CSS -->
    <link href=@Url.Content("/Content/css/jquery.smartmenus.bootstrap.css") rel="stylesheet">
    <!-- Product view slider -->
    <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/css/jquery.simpleLens.css")>
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/css/slick.css")>
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/css/nouislider.css")>
    <!-- Theme color -->
    <link id="switcher" href=@Url.Content("/Content/css/theme-color/default-theme.css") rel="stylesheet">
    <!-- <link id="switcher" href="css/theme-color/bridge-theme.css" rel="stylesheet"> -->
    <!-- Top Slider CSS -->
    <link href=@Url.Content("/Content/css/sequence-theme.modern-slide-in.css") rel="stylesheet" media="all">

    <!-- Main style sheet -->
    <link href=@Url.Content("/Content/css/style.css") rel="stylesheet">

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet" type="text/css">



    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- wpf loader Two -->
    <div id="wpf-loader-two" style="display: none;">
        <div class="wpf-loader-two-inner">
            <span>Loading</span>
        </div>
    </div>
    <!-- / wpf loader Two -->
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#" style="display: none;"><i class="fa fa-chevron-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->
    <!-- Start header section -->
    <header id="aa-header">
        <!-- start header top  -->
        <div class="aa-header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="aa-header-top-area">
                            <!-- start header top left -->
                            <div class="aa-header-top-left">



                                <!-- start cellphone -->
                                <div class="cellphone hidden-xs">
                                    <p><span class="fa fa-phone"></span>00-62-658-658</p>
                                </div>
                                <!-- / cellphone -->
                            </div>
                            <!-- / header top left -->
                            <div class="aa-header-top-right">
                                <!--data-toggle="modal"-->
                                <ul class="aa-head-top-nav-right">
                                    @if (Session["TenDangNhapSS"] != null)
                                    {
                                        <li><p>Chào mừng @Session["TenDangNhapSS"]</p></li>
                                        <li><a href="~/Views/Users/Details.cshtml">Thông tin tài khoản</a></li>
                                        <li>@Html.ActionLink("Đăng xuất", "Logout", "Home")</li>
                                    }
                                    else
                                    {
                                        <li>@Html.ActionLink("Đăng ký", "SignUp", "Home")</li>
                                        <li>@Html.ActionLink("Đăng nhập", "Login", "Home")</li>
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- / header top  -->
        <!-- start header bottom  -->
        <div class="aa-header-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="aa-header-bottom-area">
                            <!-- logo  -->
                            <img src="https://i.imgur.com/qLw1NvV.png" style=" width: 250px;">
                            <!-- cart box -->
                            <div class="aa-cartbox">
                                <a class="aa-cart-link" href="ShoppingCart/ShowCart">
                                    <span class="fa fa-shopping-basket"></span>
                                    <span class="aa-cart-title">GIỎ HÀNG</span>
                                    @Html.Action("BagCart", "ShoppingCart")
                                </a>

                            </div>
                            <!-- / cart box -->
                            <!-- search box -->
                            <div class="aa-search-box">
                                <form action="@Url.Action("Search", "Home")" method="get">
                                    <input type="text" name="Keyword" id="" placeholder="Tìm kiếm sản phẩm ">
                                    <button type="submit"><span class="fa fa-search"></span></button>
                                </form>
                            </div>
                            <!-- / search box -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- / header bottom  -->
    </header>
    <section id="menu">
        <div class="container">
            <div class="menu-area">
                <!-- Navbar -->
                <div class="navbar navbar-default" role="navigation">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="navbar-collapse collapse">
                        <!-- Left nav -->
                        <ul class="nav navbar-nav" data-smartmenus-id="16212036692596023">
                           
                                <li>
                                    <a href="#" class="has-submenu" id="sm-16212036692596023-1" aria-haspopup="true" aria-controls="sm-16212036692596023-2" aria-expanded="false"> <span class="caret"></span></a>
                                    <ul class="dropdown-menu" id="sm-16212036692596023-2" role="group" aria-hidden="true" aria-labelledby="sm-16212036692596023-1" aria-expanded="false">
                                        <li><a href="#"></a></li>
                                    </ul>
                                </li>
                            

                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </div>
        </div>

    </section>
    <section id="aa-catg-head-banner">
        <img src="~/Content/unnamed.png" />
        <div class="aa-catg-head-banner-area">
            <div class="container">
                <div class="aa-catg-head-banner-content">
                </div>
            </div>
        </div>
    </section>
    <div>
        @RenderBody()
    </div>


